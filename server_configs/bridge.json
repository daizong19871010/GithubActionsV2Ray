{
  "log": {
    "access": "/var/log/xray/bridge_access.log",
    "error": "/var/log/xray/bridge_error.log",
    "loglevel": "Warning"
  },
  "reverse": {
    "portals": [
      {
        "tag": "portal",
        "domain": "private.cloud.com"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "githubactions",
      "listen": "127.0.0.1",
      "port": 5762,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "160f2a90-9f87-4452-b27a-e4c03341c138",
            "level": 0
          }
        ],
        "decryption": "none",
        "fallbacks": []
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/githubactions"
        }
      }
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": [
          "githubactions"
        ],
        "outboundTag": "portal"
      }
    ]
  }
}
